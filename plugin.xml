<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-azure-computer-vision-plugin" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>CordovaAzureComputerVision</name>
    <js-module name="CordovaAzureComputerVision" src="www/CordovaAzureComputerVision.js">
        <clobbers target="cordova.plugins.CordovaAzureComputerVision" />
    </js-module>
    <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
    <feature name="CordovaAzureComputerVision">
        <param name="android-package" value="cordova-azure-computer-vision-plugin.CordovaAzureComputerVision" /></feature>
        </config-file><config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/CordovaAzureComputerVision.java" target-dir="src/cordova-azure-computer-vision-plugin/CordovaAzureComputerVision" />
  </platform>
</plugin>